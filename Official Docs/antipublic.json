{
  "openapi": "3.1.0",
  "info": {
    "title": "AntiPublic.One API",
    "description": "Official API documentation for AntiPublic MYRZ.\n\nLinks:\n+ Download [AntiPublic](https://antipublic.one/dl/AntiPublic.zip).\n+ AntiPublic [Management](https://lolz.live/account/antipublic).",
    "version": "1.0.8a"
  },
  "servers": [
    {
      "url": "https://antipublic.one/api/v2"
    }
  ],
  "paths": {
    "/countLines": {
      "get": {
        "tags": [
          "Info"
        ],
        "summary": "Count Lines",
        "description": "Get count of rows in the AntiPublic db.",
        "operationId": "Public.CountLines",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CountLines"
                }
              }
            }
          }
        }
      }
    },
    "/countLinesPlain": {
      "get": {
        "tags": [
          "Info"
        ],
        "summary": "Count Lines Plain",
        "description": "Get count of rows in the AntiPublic db (raw format).",
        "operationId": "Public.CountLinesPlain",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/CountLinesPlain"
                }
              }
            }
          }
        }
      }
    },
    "/version": {
      "get": {
        "tags": [
          "Info"
        ],
        "summary": "Check Updates",
        "description": "Get current version etc.",
        "operationId": "Public.Version",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Version"
                }
              }
            }
          }
        }
      }
    },
    "/checkAccess": {
      "get": {
        "tags": [
          "Account"
        ],
        "summary": "Check Access",
        "description": "Checks your license.",
        "operationId": "License.CheckLicense",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CheckAccess"
                }
              }
            }
          },
          "403": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IncorrectKey"
                }
              }
            }
          }
        },
        "security": [
          {
            "AntiPublic": []
          }
        ]
      }
    },
    "/availableQueries": {
      "get": {
        "tags": [
          "Account"
        ],
        "summary": "Get Available Queries",
        "description": "Get your available queries.",
        "operationId": "License.AvailableQueries",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AvailableQueries"
                }
              }
            }
          },
          "403": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IncorrectKey"
                }
              }
            }
          }
        },
        "security": [
          {
            "AntiPublic": []
          }
        ]
      }
    },
    "/checkLines": {
      "post": {
        "tags": [
          "Check"
        ],
        "summary": "Check Lines",
        "description": "Check your lines. Maximum 1000 lines per request.",
        "operationId": "License.CheckLines",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "lines"
                ],
                "properties": {
                  "insert": {
                    "description": "Upload private rows to AntiPublic db.",
                    "title": "Insert",
                    "type": "boolean"
                  },
                  "lines": {
                    "description": "Lines for check, email:password.",
                    "title": "Lines",
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CheckLines"
                }
              }
            }
          },
          "403": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IncorrectKey"
                }
              }
            }
          }
        },
        "security": [
          {
            "AntiPublic": []
          }
        ]
      }
    },
    "/search": {
      "post": {
        "tags": [
          "Search"
        ],
        "summary": "Search",
        "description": "Search lines by email/password/domain.",
        "operationId": "License.Search",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "Search by email",
                "required": [
                  "searchBy",
                  "query"
                ],
                "properties": {
                  "searchBy": {
                    "description": "Search type.\n\nSearch Priority:\n- **searchBy: email** - `email`, then `password`.\n- **searchBy: password** - `password`, then `email`.\n- **searchBy: domain** - `domain`, then `email`, then `password`.",
                    "title": "Search type.",
                    "type": "string",
                    "default": "email",
                    "enum": [
                      "email",
                      "password",
                      "domain"
                    ]
                  },
                  "query": {
                    "description": "Search query.",
                    "title": "Search query.",
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string"
                      },
                      "password": {
                        "type": "string"
                      },
                      "domain": {
                        "type": "string"
                      }
                    }
                  },
                  "direction": {
                    "description": "Search direction.",
                    "title": "Search direction.",
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string",
                        "enum": [
                          "start",
                          "strict"
                        ]
                      },
                      "password": {
                        "type": "string",
                        "enum": [
                          "start",
                          "strict"
                        ]
                      },
                      "domain": {
                        "type": "string",
                        "enum": [
                          "start",
                          "strict",
                          "end"
                        ]
                      }
                    }
                  },
                  "pageToken": {
                    "type": "string",
                    "title": "Page token",
                    "description": "Page token."
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmailSearch"
                }
              }
            }
          },
          "403": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NoAccess"
                }
              }
            }
          }
        },
        "security": [
          {
            "AntiPublic": []
          }
        ]
      }
    },
    "/emailPasswords": {
      "post": {
        "tags": [
          "Search"
        ],
        "summary": "Email Passwords",
        "description": "Get passwords for emails. AntiPublic Plus subscription required.",
        "operationId": "LicensePlus.EmailPasswords",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "emails"
                ],
                "properties": {
                  "emails": {
                    "description": "Emails for search.",
                    "title": "Emails",
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "limit": {
                    "description": "Result limit (per email).",
                    "title": "Limit",
                    "type": "integer",
                    "maximum": 1000
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmailSearch"
                }
              }
            }
          },
          "403": {
            "description": "Error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NoAccess"
                }
              }
            }
          }
        },
        "security": [
          {
            "AntiPublic": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "CountLines": {
        "type": "object",
        "properties": {
          "count": {
            "type": "integer"
          }
        }
      },
      "CountLinesPlain": {
        "type": "string"
      },
      "Version": {
        "type": "object",
        "properties": {
          "filename": {
            "type": "string",
            "default": "AntiPublic.exe"
          },
          "version": {
            "type": "string"
          },
          "changeLog": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        }
      },
      "CheckAccess": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "plus": {
            "type": "boolean"
          },
          "trial": {
            "type": "boolean",
            "default": false
          },
          "plus_expires": {
            "type": "string",
            "default": "0"
          },
          "trial_expires": {
            "type": "string",
            "default": "0"
          }
        }
      },
      "IncorrectKey": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "default": false
          },
          "error": {
            "type": "string",
            "default": "Некорректный ключ"
          }
        }
      },
      "AvailableQueries": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "emailSearch": {
            "type": "integer"
          },
          "passwordSearch": {
            "type": "integer"
          }
        }
      },
      "NoAccess": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "default": false
          },
          "error": {
            "type": "string",
            "default": "Отсутствует доступ"
          }
        }
      },
      "CheckLines": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "default": true
          },
          "result": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LineResult"
            }
          }
        }
      },
      "LineResult": {
        "type": "object",
        "properties": {
          "is_private": {
            "type": "boolean",
            "default": false
          },
          "line": {
            "type": "string",
            "default": "example@gmail.com:password"
          }
        }
      },
      "EmailSearch": {
        "type": "object",
        "properties": {
          "success": {
            "title": "Success",
            "type": "boolean"
          },
          "availableQueries": {
            "title": "Available queries",
            "type": "integer"
          },
          "resultCount": {
            "title": "Result count",
            "type": "integer"
          },
          "results": {
            "title": "Results",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "page": {
            "title": "Page",
            "type": "object",
            "properties": {
              "totalResults": {
                "title": "Total results",
                "type": "integer"
              },
              "pageNumber": {
                "title": "Page number",
                "type": "integer"
              },
              "nextPageToken": {
                "title": "Next page token",
                "type": "string"
              }
            },
            "required": [
              "totalResults",
              "pageNumber",
              "nextPageToken"
            ]
          }
        },
        "required": [
          "success",
          "availableQueries",
          "resultCount",
          "results",
          "page"
        ]
      }
    },
    "securitySchemes": {
      "AntiPublic": {
        "type": "apiKey",
        "name": "key",
        "in": "query"
      }
    }
  },
  "x-readme": {
    "explorer-enabled": true,
    "proxy-enabled": true,
    "samples-enabled": true
  }
}